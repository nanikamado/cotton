fib : I64 -> I64 =
    | 0 => 0
    | 1 => 1
    | n => (n - 1).fib + (n - 2).fib

test : I64 -> String =
    | a { (| x => x + a) : a } { (| x => x + a) : 30 } => a.to_string <> "!"
    | a => a.to_string

main : () -> () =
    | () => do
        2.test.puts
        10.test.puts
        20.test.puts
