interface ToString where
    to_string : Self -> String

infixl 10 .

data LocalNil
data A /=\ B forall { A, B }

infixr 5 /=\

type LocalList = LocalNil | A /=\ List[A] forall { A }

list_to_string_rec : LocalList[A] -> String forall { A : ToString } =
    | LocalNil => ""
    | h /=\ t => ", " <> h.to_string <> t.list_to_string_rec

to_string : List[A] -> String forall { A : ToString } =
    | LocalNil => "[]"
    | h /=\ t => "[" <> h.to_string <> t.list_to_string_rec <> "]"

main = | () => do
    ((() /=\ LocalNil) /=\ LocalNil).to_string
    ()
