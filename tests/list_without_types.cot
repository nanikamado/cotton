(.)
= | a, f => f(a)

infixl 10 .

data A /\ B forall { A, B }

infixl 3 /\

data A .. B forall { A, B }

infixl 4 ..

range_to_list =
    | a .. b => (a < b).
        | True => a /\ ((a + 1)..b).range_to_list
        | False => ()

print_num_list =
    | () => ()
    | h /\ t => do
        h.i64_to_string.println
        t.print_num_list

main : () -> () =
    | () => (0..100).range_to_list.print_num_list
