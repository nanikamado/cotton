data Nil
data A /\ B forall { A, B }

infixr 5 /\

type List = Nil | A /\ List[A] forall { A }

list_to_string_rec : List[A] -> String forall { A : ToString } =
    | Nil => ""
    | h /\ t => ", " ++ h.to_string ++ t.list_to_string_rec

to_string : List[A] -> String forall { A : ToString } =
    | Nil => "[]"
    | h /\ t => "[" ++ h.to_string ++ t.list_to_string_rec ++ "]"

main = | () => do
    (("a" /\ Nil) /\ Nil).to_string.println
    ()
