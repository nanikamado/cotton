infixl 10 .
infixr 3 /\

fold : List[A] -> B -> (B -> A -> B) -> B forall { A, B } =
    | Nil, init, _ => init
    | h /\ t, init, f => t.fold(init.f(h), f)

main : () -> () =
    | () => do
        (1 /\ 2 /\ 3 /\ 4 /\ 5 /\ Nil)
            .fold(0, (+))
            .i64_to_string
            .println
        (1 /\ 2 /\ 3 /\ 4 /\ 5 /\ Nil)
            .fold(1, (*))
            .i64_to_string
            .println
