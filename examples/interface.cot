data Hoge

greet : Hoge -> String =
    | _ => "Hogeeeeee"

data Fuga

greet : Fuga -> String =
    | _ => "Fugaaaaaa"

interface Greet {
    greet : Self -> String
}

greet_twice : A -> String forall { A : Greet } =
    | a => do 
        a.greet <> a.greet

main : () -> () =
    | () => do
        Hoge.greet_twice.println
        Fuga.greet_twice.println
