data Tuple(a, b)

data a /\ b

fizzbuzz : I32 -> String
= fn
| n =>
    Tuple(n % 3, n % 5).fn
    | Tuple(0, 0) => "FizzBuzz"
    | Tuple(0, _) => "Fizz"
    | Tuple(_, 0) => "Buzz"
    | Tuple(_, _) => n
    --
--

fizzbuzz_loop : I32 -> ()
= fn
| 0 => ()
| n =>
    fizzbuzz_loop(n - 1)
    n.fizzbuzz.println
--

(.) : Any
= fn
| a, f => f(a)
--

main : () -> ()
= fn
| () =>
    100.fizzbuzz_loop
--